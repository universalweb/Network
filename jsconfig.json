{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"#certificate": [
				"udsp/certificate/index.js"
			],
			"#cleanPath": [
				"./utilities/cleanPath.js"
			],
			"#client*": [
				"udsp/client/index.js"
			],
			"#client/*": [
				"udsp/client/*.js"
			],
			"#components/*": [
				"./components/*.js"
			],
			"#config/*": [
				"./config/*.js"
			],
			"#crypto/": [
				"./utilities/cryptography/"
			],
			"#hash/": [
				"./utilities/cryptography/hash/*.js"
			],
			"#cipher/": [
				"./utilities/cryptography/cipher/*.js"
			],
			"#keyExchange/": [
				"./utilities/cryptography/keyExchange/*.js"
			],
			"#signature/": [
				"./utilities/cryptography/signature/*.js"
			],
			"#examples/*": [
				"./examples/*.js"
			],
			"#file": [
				"./utilities/file.js"
			],
			"#keychain": [
				"udsp/certificate/keychain.js"
			],
			"#logs": [
				"utilities/logs/logs.js"
			],
			"#pluckBuffer": [
				"./utilities/pluckBuffer.js"
			],
			"#profile/*": [
				"cryptoID/*.js"
			],
			"#propertyAccess": [
				"./utilities/propertyAccess.js"
			],
			"#resources/*": [
				"./resources/*"
			],
			"#schemes/*": [
				"browser/schemes/*.js"
			],
			"#server*": [
				"udsp/server/index.js"
			],
			"#server/*": [
				"udsp/server/*.js"
			],
			"#serverApp/*": [
				"examples/serverApp/*.js"
			],
			"#sodium": [
				"./sodium.js"
			],
			"#state/*": [
				"./state/*.js"
			],
			"#udsp": [
				"./udsp/index.js"
			],
			"#udsp/*": [
				"./udsp/*.js"
			],
			"#utilities/*": [
				"./utilities/*.js"
			],
			"#viat/*": [
				"./viat/*.js"
			],
			"#watch": [
				"./utilities/watch.js"
			]
		},
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"target": "ESNext",
		"jsx": "react",
		"strictNullChecks": true,
		"experimentalDecorators": true
	},
	"exclude": [
		"**/build/*",
		"**/docSource/*",
		"**/examples/*",
		"**/node_modules/*",
		"**/npm/*",
		"**/package/*",
		"**/target/*",
		"node_modules"
	]
}
