{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"#utilities/*": [
				"./utilities/*.js"
			],
			"#server/*": [
				"udsp/server/*.js"
			],
			"#client/*": [
				"udsp/client/*.js"
			],
			"#server*": [
				"udsp/server/index.js"
			],
			"#client*": [
				"udsp/client/index.js"
			],
			"#profile/*": [
				"./profile/*.js"
			],
			"#serverApp/*": [
				"./serverApp/*.js"
			],
			"#schemes/*": [
				"./schemes/*.js"
			],
			"#resources/*": [
				"./resources/*"
			],
			"#scripts/*": [
				"./scripts/*.js"
			],
			"#state/*": [
				"./state/*.js"
			],
			"#config/*": [
				"./config/*.js"
			],
			"#udsp/*": [
				"./udsp/*.js"
			],
			"#sodium": [
				"./sodium.js"
			],
			"#crypto": [
				"./utilities/crypto.js"
			],
			"#watch": [
				"./utilities/watch.js"
			],
			"#logs": [
				"./utilities/logs.js"
			],
			"#cleanPath": [
				"./utilities/cleanPath.js"
			],
			"#file": [
				"./utilities/file.js"
			],
			"#propertyAccess": [
				"./utilities/propertyAccess.js"
			],
			"#pluckBuffer": [
				"./utilities/pluckBuffer.js"
			],
			"#certificate": [
				"./utilities/certificate/index.js"
			],
			"#udsp": [
				"./udsp/index.js"
			],
			"#keychain": [
				"./utilities/keychain.js"
			]
		},
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"target": "ESNext",
		"jsx": "react",
		"strictNullChecks": true,
		"experimentalDecorators": true
	},
	"exclude": [
		"**/build/*",
		"**/docSource/*",
		"**/docs/*",
		"**/examples/*",
		"**/node_modules/*",
		"**/npm/*",
		"**/package/*",
		"**/target/*",
		"node_modules"
	]
}