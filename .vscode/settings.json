{
	"editor.cursorBlinking": "solid",
	"editor.formatOnType": false,
	"editor.autoClosingOvertype": "always",
	"editor.hideCursorInOverviewRuler": true,
	"editor.tabCompletion": "on",
	"editor.cursorSmoothCaretAnimation": "on",
	"cSpell.words": [
		"ABYTES",
		"cnsl",
		"esformatter",
		"evnt",
		"msgpackr",
		"NPUBBYTES",
		"pwhash",
		"qrcode",
		"universalweb",
		"uw",
		"xchacha"
	],
	"cSpell.enabledFiletypes": [
		"md",
		"json",
		"js"
	],
	"sarif-viewer.connectToGithubCodeScanning": "off",
	"cSpell.customDictionaries": {
		"custom-dictionary-workspace": {
			"name": "custom-dictionary-workspace",
			"path": "${workspaceFolder:Network}/.cspell/custom-dictionary-workspace.txt",
			"addWords": true,
			"scope": "workspace"
		}
	},
	"docwriter.style": "JSDoc",
	"diffEditor.codeLens": true,
	"MutableAI.upsell": false,
	"todo-tree.tree.scanMode": "workspace only",
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": "keyword.control.trycatch.js",
				"settings": {
					"foreground": "#55ebff",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "keyword.operator.ternary.js",
				"settings": {
					"foreground": "#d587ff",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "constant.language.boolean.true.js",
				"settings": {
					"foreground": "#10ff6c",
					"fontStyle": "italic bold"
				}
			},
			{
				"scope": "variable.other.readwrite.alias.js",
				"settings": {
					"foreground": "#ffb1ea",
					"fontStyle": "italic bold"
				}
			},
			{
				"scope": "constant.language.boolean.false.js",
				"settings": {
					"foreground": "#ff1f1f",
					"fontStyle": "italic bold"
				}
			},
			{
				"scope": "storage.type.js",
				"settings": {
					"foreground": "#bcffff",
					"fontStyle": "italic bold"
				}
			},{
				"scope": "keyword.operator.assignment.js",
				"settings": {
					"foreground": "#b1b1b1",
					"fontStyle": ""
				}
			},{
				"scope": "keyword.operator.logical.js",
				"settings": {
					"foreground": "#baff90",
					"fontStyle": ""
				}
			},
			{
				"scope": "keyword.control.export.js",
				"settings": {
					"foreground": "#FF7F50",
					"fontStyle": "italic"
				}
			},
			{
				"scope": "keyword.control.from.js",
				"settings": {
					"foreground": "#ff5398",
					"fontStyle": "italic"
				}
			},
			{
				"scope": "keyword.control.import.js",
				"settings": {
					"foreground": "#ff5398",
					"fontStyle": "italic"
				}
			},
			{
				"scope": "keyword.control.default.js",
				"settings": {
					"foreground": "#51e081",
					"fontStyle": "bold italic"
				}
			},
			{
				"scope": "storage.modifier.async.js",
				"settings": {
					"foreground": "#f64747",
					"fontStyle": "bold underline"
				}
			},
			{
				"scope": "keyword.control.flow.js",
				"settings": {
					"foreground": "#ff004c",
					"fontStyle": "underline"
				}
			},
			{
				"scope": "constant.language.null.js",
				"settings": {
					"foreground": "#D8BFD8",
					"fontStyle": ""
				}
			},
			{
				"scope": "string.quoted.single",
				"settings": {
					"foreground": "#fff",
					"fontStyle": ""
				}
			},
			{
				"scope": "string.quoted.double",
				"settings": {
					"foreground": "#fff",
					"fontStyle": ""
				}
			},
			{
				"scope": "punctuation.accessor.optional.js",
				"settings": {
					"foreground": "#d3b2ff",
					"fontStyle": "bold"
				}
			},
			{
				"scope": "variable.parameter.js",
				"settings": {
					"fontStyle": ""
				}
			},
			{
				"scope": "punctuation.separator",
				"settings": {
					"foreground": "#c0c0c0",
					"fontStyle": ""
				}
			},
			{
				"scope": "punctuation.definition.string.begin.js",
				"settings": {
					"foreground": "#a4a4a4",
					"fontStyle": ""
				}
			},
			{
				"scope": "punctuation.definition.string.end.js",
				"settings": {
					"foreground": "#a4a4a4",
					"fontStyle": ""
				}
			},
			{
				"scope": "storage.type.function.js",
				"settings": {
					"foreground": "#43ffff",
					"fontStyle": "italic"
				}
			},
			{
				"scope": "storage.type.class.js",
				"settings": {
					"foreground": "#ff88f5",
					"fontStyle": "italic"
				}
			},
			{
				"scope": "variable.language.this",
				"settings": {
					"foreground": "#ffb459",
					"fontStyle": ""
				}
			}
		],
		"[*Light*]": {
			"textMateRules": [
				{
					"scope": "ref.matchtext",
					"settings": {
						"foreground": "#000"
					}
				}
			]
		},
		"[*Dark*]": {
			"textMateRules": [
				{
					"scope": "ref.matchtext",
					"settings": {
						"foreground": "#fff"
					}
				}
			]
		}
	},
	"errorLens.enabledDiagnosticLevels": [
		"error",
		"warning",
		"info",
		"hint"
	],
	"explorer.copyRelativePathSeparator": "/",
	"files.associations": {
		"*.html": "html",
		"*.block": "cbor",
		"*.hbs": "handlebars",
		"*.umm": "javascript",
		"__bit_reference": "cpp",
		"__hash_table": "cpp",
		"__locale": "cpp",
		"__split_buffer": "cpp",
		"array": "cpp",
		"atomic": "cpp",
		"bitset": "cpp",
		"complex": "cpp",
		"cstddef": "cpp",
		"cstdio": "cpp",
		"cstdlib": "cpp",
		"cstring": "cpp",
		"ctime": "cpp",
		"cwchar": "cpp",
		"deque": "cpp",
		"memory": "cpp",
		"format": "cpp",
		"initializer_list": "cpp",
		"ios": "cpp",
		"istream": "cpp",
		"iterator": "cpp",
		"locale": "cpp",
		"mutex": "cpp",
		"new": "cpp",
		"optional": "cpp",
		"print": "cpp",
		"string": "cpp",
		"string_view": "cpp",
		"tuple": "cpp",
		"type_traits": "cpp",
		"typeinfo": "cpp",
		"unordered_map": "cpp",
		"utility": "cpp",
		"variant": "cpp",
		"vector": "cpp",
		"string.h": "c",
		"stdlib.h": "c"
	},
	
}
